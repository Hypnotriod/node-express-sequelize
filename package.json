{
  "name": "express-test-01",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {
    "launch": "node build/index.js",
    "relaunch": "tsc && node build/index.js",
    "debug": "nodemon --inspect --debug-port=9229 build/index.js",
    "debug-brk": "nodemon --inspect-brk --debug-port=9229 build/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "launch-debug": "concurrently --kill-others \"npm run debug\" \"npm run watch\"",
    "launch-debug-brk": "concurrently --kill-others \"npm run debug-brk\" \"npm run watch\""
  },
  "author": "Ilya Pikin",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.6.15",
    "@overnightjs/logger": "^1.1.10",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "pg": "^8.0.2",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.6",
    "sequelize-typescript": "^1.1.0",
    "tsyringe": "^4.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/express": "^4.17.6",
    "@types/node": "^13.11.1",
    "@types/validator": "^13.0.0",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.3",
    "typescript": "^3.8.3"
  }
}
